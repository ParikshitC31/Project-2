#include<iostream>
using namespace std;
int main()
{
    int age;
    cout<<" Enter your age:  ";
    cin>>age;
    if(age>=18)
    { 
        cout<<"You are eligible for voting, just you need your voter id card."<< "\n ENJOY VOTING!!";
    }    
    else
    {

        cout<<" You can't vote now."<<"\n You seems to be a TEENAGER";
        cout<<",you have to wait for "<<18-age<<" more year's." ;
    }

    
    
    return 0;
}
